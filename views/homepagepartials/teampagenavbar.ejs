        <a href="/">
          <img class="site-logo" src="<%= team.image.path %>" alt="Welcome to Victory Theme FC" />
        </a>
        <div class="site-title">
          <h1 class="site-name" itemprop="headline">
            <a href="/" title="Victory Theme" rel="home"><%= team.name %></a>
