<div id="page" class="hfeed site">
  <a class="skip-link screen-reader-text" href="#content">Skip to content</a>
  <div class="top-menu">
    <div class="inside">
      <div class="top-menu-wrap">
        <div class="date-time-display">
          Monday, April 1 </div>
        <div class="weather-display">
          <i class="weather-icon climacons climacon-clouds"></i>
          <div class="weather-place">London, UK</div>
          <div class="weather-temperature">8.9<span class="weather-unit">&deg;C</span></div>
        </div>
        <div id="top-nav" class="menu-top-container">
          <ul id="menu-top" class="menu">
            <li id="menu-item-1580" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-1580"><a href="#">News</a></li>
